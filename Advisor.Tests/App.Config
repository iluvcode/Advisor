<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>

    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
  </configSections>
  <connectionStrings>
    <clear />
    
    <add name="DefaultConnection" connectionString="server=.\mssql2012;Initial Catalog=HomeAdvisor;Integrated Security=true;" />
    <add name="DefaultConnectionReadOnly" connectionString="server=.\mssql2012;Initial Catalog=HomeAdvisor;Integrated Security=true;" />
  </connectionStrings>
  <appSettings>
    <clear />
    <add key="ApplicationConnectionStringName" value="DefaultConnection" />
    <add key="ApplicationReadOnlyConnectionStringName" value="DefaultConnectionReadOnly" />
    <add key="IsRegistryVew32Bit" value="false" />
    <add key="EnableInternalStore" value="false" />
    <add key="IsDatabaseOnHACluster" value="false" />
    <add key="ConnectionStringKeyRegistryKeyNamePrivate" value="ConnEncryptionPrivateKey" />
    <add key="ConnectionStringKeyRegistryKeyNamePublic" value="ConnEncryptionPublicKey" />
    <add key="AppNameRegistryKey" value="SmartSite\MPW" />

  </appSettings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <runtime>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>
